# Eva API Configuration  
# URL of your Eva-project API  
EVA_API_URL=https://eva.s7corp.ru/api  
  
# API token for Eva API access  
# Get token from Eva-project profile settings  
EVA_API_TOKEN=your_token_here  
  
# Read-only mode (default: true)  
# Set to false to allow write operations (create/update tasks, comments)  
EVA_READ_ONLY=true  
  
# Request timeout in seconds (default: 30)  
EVA_TIMEOUT=30 
